/******************************************************************************

             Count digits in an number
*******************************************************************************/

public class Main
{
	public static void main(String[] args) {
		System.out.println("Hello World");
	    int n=954234563;
	    int c=0;
	   
	        while(n!=0)
	        {
	        n=n/10;
	        c++;
	        }
	    
	    System.out.println(c);
		   
		
	}
}
